  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Frontend in Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173/",
      "webRoot": "${workspaceFolder}/apps/frontend",
      "preLaunchTask": "start-frontend"
    },
    {
      "name": "Debug Auth Service",
      "type": "node",
      "request": "attach",
      "port": 1111
    },
    {
      "name": "Debug User Service",
      "type": "node",
      "request": "attach",
      "port": 1112
    },
    {
      "name": "Debug Projects Service",
      "type": "node",
      "request": "attach",
      "port": 1113
    },
    {
      "name": "Debug Documents Service",
      "type": "node",
      "request": "attach",
      "port": 1114
    }
  ]
}
